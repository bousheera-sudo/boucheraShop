{
  "version": 2,
  "functions": {
    "api/lambda.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "^/css/(.*)$", "dest": "/public/css/$1" },
    { "src": "^/imgs/(.*)$", "dest": "/public/imgs/$1" },
    { "src": "^/js/(.*)$", "dest": "/public/js/$1" },
    { "src": "^/fonts/(.*)$", "dest": "/public/fonts/$1" },
    {
      "src": "^/(.*)$",
      "dest": "/api/lambda.php"
    }
  ]
}
